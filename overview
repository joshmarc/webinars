            let ensightenCountryData = "CN";
            let ensightenUrlData = "CN:\/\/nexus.ensighten.com\/wyndham\/wyndhamhotels\u002Dchina\u002Dprod\/Bootstrap.js";
            function getEnsightenCookie(name) {
				let nameEQ = name + '=';
				let ca = document.cookie.split(';');
				for (let i = 0; i < ca.length; i++) {
				  let c = ca[i];
				  while (c.charAt(0) == ' ') c = c.substring(1, c.length);
				  if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
				}
				return null;
			  }
            let countrycookie = getEnsightenCookie("country_code");
            let ensightenCountries = ensightenCountryData.split(",");
            let ensightenUrls = ensightenUrlData.split(",");
